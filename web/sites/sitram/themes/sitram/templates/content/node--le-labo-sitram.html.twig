                      <td>{{ content.field_labo_section_3_1_col_3[2]|raw }}</td>
                    </tr>
                    <tr class="bg-verylightgrey">
                      <td>{{ content.field_labo_section_3_1_col_1[3]|raw }}</td>
                      <td>{{ content.field_labo_section_3_1_col_2[3]|raw }}</td>
                      <td>{{ content.field_labo_section_3_1_col_3[3]|raw }}</td>
                    </tr>
                    <tr>
                      <td>{{ content.field_labo_section_3_1_col_1[4]|raw }}</td>
                      <td>{{ content.field_labo_section_3_1_col_2[4]|raw }}</td>
                      <td>{{ content.field_labo_section_3_1_col_3[4]|raw }}</td>
                    </tr>
                    <tr class="bg-verylightgrey">
                      <td>{{ content.field_labo_section_3_1_col_1[5]|raw }}</td>
                      <td>{{ content.field_labo_section_3_1_col_2[5]|raw }}</td>
                      <td>{{ content.field_labo_section_3_1_col_3[5]|raw }}</td>
                    </tr>
                    <tr>
                      <td>{{ content.field_labo_section_3_1_col_1[6]|raw }}</td>
                      <td>{{ content.field_labo_section_3_1_col_2[6]|raw }}</td>
                      <td>{{ content.field_labo_section_3_1_col_3[6]|raw }}</td>
                    </tr>
                    </tbody>
                  </table>

                </div>
              </div>

            </div>

          </div>

        </div>
      </div>
    </div>

  </div>

  <!-- SECTION 4 -->
  <div class="container-fluid mt-5 pt-5" id="section_4">

    <div class="row">
      <div class="container">
        <div class="row">
          <div class="col">
            <h2 class="txt-blue mb-5">{{ content.field_labo_section_4_titre }}</h2>
          </div>
        </div>
        <div class="row">

          {{ content.field_labo_section_4_videos }}

        </div>
        <div class="row py-5 text-center">

          <a href="" class="btn_labo">
            Voir tous les tests
          </a>

        </div>
      </div>
    </div>

  </div>

</div>
