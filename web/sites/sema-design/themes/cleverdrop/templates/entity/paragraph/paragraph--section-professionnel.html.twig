{% set classes = [
  'Profesional-wrp'
] %}
{% block paragraph %}
  {% if content.title is not empty or content.field_description[0] is not empty %}
  <div{{ attributes.addClass(classes) }}>
    {% block content %}
      <div class="Section">
        <div class="Section-inner Profesional">
          <div class="Particular-left">
            {{ content.field_title }}
          </div>
          <div class="Profesional-right">
            {{ content.field_description }}
          </div>
        </div>
      </div>
    {% endblock %}
  </div>
  {% endif %}
{% endblock paragraph %}
