<!-- CONTENT 3 -->
<div class="qsn-content-3">
    <div class="container">
        <div class="row">
            <div class="col-12 col-sm-10 offset-sm-1 text-center intro-txt">
                {{ content.field_qsn_content3_intro }}
            </div>
        </div>
        <div class="row blocks-txt">
            <div class="col-12 col-sm-6">
                <div class="inner">
                    {{ content.field_qsn_content3_blocktxt[0] }}
                </div>
            </div>
            <div class="col-12 col-sm-6">
                <div class="inner">
                    {{ content.field_qsn_content3_blocktxt[1] }}
                </div>
