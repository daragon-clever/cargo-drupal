<!-- CONTENT 4 -->
<div class="qsn-content-4">
    <div class="container">
        <div class="row">
            <div class="col-12 col-sm-8 offset-sm-2 text-center intro-txt">
                {{ content.field_qsn_content4_intro }}
            </div>
        </div>
        <div class="row p1">
            <div class="col-12">
                <span class="part-title">
                    {{ content.field_qsn_content4_p1_title }}
                </span>
                <div class="container-sub">
                    <div class="row justify-content-center">
                    {% for i in 0..7 %}
                        <div class="col-6 col-sm-3 text-center block">
                            <div class="svg-wrapper">
                                {{ content.field_qsn_content4_p1_svg[i] }}
                            </div>
                            <strong>{{ i + 1 }}.</strong>
                            {{ content.field_qsn_content4_p1_txt[i] }}
                        </div>
                    {% endfor %}
                    </div>
                </div>
            </div>
        </div>

        <div class="row p2">
            <div class="col-12">
                <span class="part-title">
                    {{ content.field_qsn_content4_p2_title }}
                </span>
                <div class="row justify-content-center">
                    {% for i in 0..5 %}
                        <div class="col-auto text-center block">
                            <div class="svg-wrapper">
                                {{ content.field_qsn_content4_p2_svg[i] }}
                            </div>
                            <span>{{ content.field_qsn_content4_p2_txt[i] }}</span>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>
</div>